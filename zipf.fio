;[global]
;filename=dajfl
;direct=1
;thread
;numjobs=10
[job]
thread
numjobs=8
iodepth=32
ioengine=kvdb
kvdb_name=/dev/sdb4
disable_cache=0
cache_size=1024
cache_isDedup=0
cache_policy=0
slru_partition=50
rw=randread
norandommap
;allrandrepeat=1
size=1G
bs=4k
random_distribution=zipf:1.2
time_based=1
runtime=20
;fallocate=0

group_reporting
